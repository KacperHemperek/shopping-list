{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2016",                               
    "esModuleInterop": true,                          
    "forceConsistentCasingInFileNames": true,         
    "skipLibCheck": true,     
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
               
  }
}
